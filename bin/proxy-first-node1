#!/usr/bin/env python3

import sys
from twisted.python import log

sys.path.append('.')

from cpchain.utils import reactor
from cpchain.proxy.kadnet import KadNode
from cpchain.proxy.centralnet import Tracker

log.startLogging(sys.stdout)

#kad_node = KadNode()
#kad_node.first_node()

tracker = Tracker()
tracker.run()

try:
    reactor.run()
except KeyboardInterrupt:
    pass
finally:
    # kad_node.stop()
    tracker.stop()
